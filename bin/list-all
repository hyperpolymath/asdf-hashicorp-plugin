# asdf-hashicorp: List all installable versions

set -euo pipefail

PLUGIN_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")/.." && pwd)"
# shellcheck source=lib/utils.bash
source "${PLUGIN_DIR}/lib/utils.bash"

check_dependencies
validate_tool "${TOOL_NAME}"
list_all_versions
